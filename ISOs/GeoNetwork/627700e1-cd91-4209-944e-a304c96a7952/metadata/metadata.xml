        <gmd:CI_Citation>
          <gmd:title>
            <gco:CharacterString>IMPACT OF FLUCTUATIONS IN WATER LEVELS AND FLOW RATES ON THE WATER USES WITHIN THE UPPER GREAT LAKES, October 12, 2008</gco:CharacterString>
          </gmd:title>
          <gmd:date>
            <gmd:CI_Date>
              <gmd:date>
                <gco:Date>2008-10-12</gco:Date>
              </gmd:date>
