          <gmd:title>
            <gco:CharacterString>Assessment of the Present and Future Water Use in the Great Lakes Basin - Final Report</gco:CharacterString>
          </gmd:title>
          <gmd:date>
            <gmd:CI_Date>
              <gmd:date>
                <gco:Date>2010-05-03</gco:Date>
              </gmd:date>
